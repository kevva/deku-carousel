{
  "name": "deku-carousel",
  "version": "1.2.2",
  "description": "Carousel component for deku",
  "license": "MIT",
  "repository": "kevva/deku-carousel",
  "author": {
    "name": "Kevin MÃ¥rtensson",
    "email": "kevinmartensson@gmail.com",
    "url": "github.com/kevva"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify -o example/build.js -d example/index.js",
    "prepublish": "babel --out-file dist.js index.js ",
    "test": "xo"
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ],
  "keywords": [
    "carousel",
    "component",
    "deku",
    "deku-component",
    "swipe"
  ],
  "dependencies": {
    "classnames": "^2.2.3",
    "deku": "2.0.0-rc16",
    "swipe": "^1.7.7"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.9.0",
    "redux": "^3.3.1",
    "xo": "*"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "quote-props": 0
    }
  }
}
